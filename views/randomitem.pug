extends layout

block content
  h1= title
  - var I = ["1.jfif", "2.jfif", "3.jfif", "4.jfif", "5.jfif"]
  - for (let i = 0; i < 3; i++)
    - var ran = Math.floor(Math.random()* 5)
    - var selected_image = I[ran]
    img(src=`/images/${selected_image}`, `alt=Random Image ${i+1}`, height="300", width="300")